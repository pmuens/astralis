{
  "name": "template-evm-dapp",
  "version": "0.1.0",
  "description": "Monorepo Template for EVM-based dApps.",
  "license": "MIT",
  "homepage": "https://github.com/pmuens/template-evm-dapp#readme",
  "repository": {
    "type": "git",
    "url": "git@github-pmuens:pmuens/template-evm-dapp.git"
  },
  "bugs": {
    "url": "https://github.com/pmuens/template-evm-dapp/issues"
  },
  "author": {
    "name": "Philipp Muens",
    "url": "https://github.com/pmuens"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "smart-contracts",
    "dapp",
    "hardhat",
    "next"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -pi run build",
    "clean": "yarn workspaces foreach -pi run clean",
    "coverage": "yarn workspaces foreach -pi run coverage",
    "dev": "yarn workspaces foreach -pi run dev",
    "format": "yarn workspaces foreach -pi run format",
    "lint": "yarn workspaces foreach -pi run lint",
    "metrics": "yarn workspaces foreach -pi run metrics",
    "test": "yarn workspaces foreach -pi run test",
    "hardhat:build": "yarn workspace hardhat run build",
    "hardhat:clean": "yarn workspace hardhat run clean",
    "hardhat:coverage": "yarn workspace hardhat run coverage",
    "hardhat:deploy": "yarn workspace hardhat run deploy",
    "hardhat:dev": "yarn workspace hardhat run dev",
    "hardhat:export": "yarn workspace hardhat run export",
    "hardhat:format": "yarn workspace hardhat run format",
    "hardhat:lint": "yarn workspace hardhat run lint",
    "hardhat:metrics": "yarn workspace hardhat run metrics",
    "hardhat:test": "yarn workspace hardhat run test",
    "hardhat:verify": "yarn workspace hardhat run verify"
  },
  "packageManager": "yarn@3.1.1"
}
